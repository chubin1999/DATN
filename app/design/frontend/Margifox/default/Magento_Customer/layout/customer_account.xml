<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Customer My Account (All Pages)" design_abstraction="custom">
    <body>
        <referenceBlock name="sidebar.additional" remove="true"/>

        <!-- Downloadable product link -->
        <referenceBlock name="customer-account-navigation-downloadable-products-link" remove="true"/>

        <!-- Billing agreement link -->
        <referenceBlock name="customer-account-navigation-billing-agreements-link" remove="true"/>

        <!-- My credit card link -->
        <referenceBlock name="customer-account-navigation-my-credit-cards-link" remove="true"/>

        <!-- Order by SKU -->
        <referenceBlock name="customer-account-navigation-checkout-sku-link" remove="true"/>

        <!-- Gift registry -->
        <referenceBlock name="customer-account-navigation-giftregistry-link" remove="true"/>

        <!-- To Remove My Invitations -->
        <referenceBlock name="customer-account-navigation-magento-invitation-link-container" remove="true"/>

        <!-- Gift card link -->
        <referenceBlock name="customer-account-navigation-gift-card-link" remove="true"/>

        <!-- Reward points -->
        <referenceBlock name="customer-account-navigation-reward-link" remove="true"/>

        <!-- Product review link -->
        <referenceBlock name="customer-account-navigation-product-reviews-link" remove="true"/>

        <referenceBlock name="customer-account-navigation-delimiter-2" remove="true"/>

        <referenceBlock name="customer-account-navigation-delimiter-1" remove="true"/>

        <referenceBlock name="customer-account-navigation-quotes-link" remove="true"/>

        <referenceBlock name="customer-account-navigation-newsletter-subscriptions-link" remove="true"/> 

        <referenceBlock name="company-credit-customer-account-navigation-delimiter-b2b" remove="true"/> 

        <referenceBlock name="customer-account-navigation-delimiter-b2b" remove="true"/> 

        <referenceBlock name="customer-account-navigation-account-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">My Dashboard</argument>
                <argument name="sortOrder" xsi:type="number">250</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">dashboard</item>
                </argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-account-edit-link">
            <arguments>
                <argument name="sortOrder" xsi:type="number">249</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">info</item>
                </argument>
            </arguments>
        </referenceBlock> 
        <referenceBlock name="customer-account-navigation-address-link">
            <arguments>
                <argument name="sortOrder" xsi:type="number">248</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">address</item>
                </argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="company-customer-account-navigation-orders-link">
            <arguments>
                <argument name="sortOrder" xsi:type="number">247</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">order</item>
                </argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-customer-balance-link"> 
            <arguments>
                <argument name="sortOrder" xsi:type="number">244</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">store-credit</item>
                </argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-brand-loyalty-rewards"> 
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Loyalty Rewards</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">loyalty-rewards</item>
                </argument>
                <argument name="sortOrder" xsi:type="number">242</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-brand-promo-rewards"> 
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Promotional Allocations</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">promo-rewards</item>
                </argument>
                <argument name="sortOrder" xsi:type="number">241</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="customer-account-navigation-company-link"> 
            <arguments>
                <argument name="sortOrder" xsi:type="number">238</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">structure</item>
                </argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-company-users-link"> 
            <arguments>
                <argument name="sortOrder" xsi:type="number">237</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">user</item>
                </argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-company-profile-link"> 
            <arguments>
                <argument name="sortOrder" xsi:type="number">240</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">company-profile</item>
                </argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-company-credit-history-link"> 
            <arguments>
                <argument name="sortOrder" xsi:type="number">239</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">credit-company</item>
                </argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-company-roles-link"> 
            <arguments>
                <argument name="sortOrder" xsi:type="number">236</argument>
                <argument name="attributes" xsi:type="array">
                   <item name="class" xsi:type="string">company-roles</item>
                </argument>
            </arguments>
        </referenceBlock>
        
        <referenceBlock name="customer_account_navigation">
            <block class="Eway\EwayRapid\Block\Mycards\Link" name="customer-account-navigation-ewayrapid-creditcards">
                <arguments>
                    <argument name="path" xsi:type="string">ewayrapid/mycards</argument>
                    <argument name="label" xsi:type="string" translate="true">Saved Cards (eWAY)</argument>
                    <argument name="attributes" xsi:type="array">
                       <item name="class" xsi:type="string">ewayrapid-creditcards</item>
                    </argument>
                    <argument name="sortOrder" xsi:type="number">243</argument>
                </arguments>
            </block>

            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-custom-logout" after="-">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Logout</argument>
                    <argument name="path" xsi:type="string">customer/account/logout</argument>
                    <argument name="sortOrder" xsi:type="number">0</argument>
                    <argument name="attributes" xsi:type="array">
                        <item name="class" xsi:type="string">logout</item>
                    </argument>
                </arguments>
            </block>
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-express-orders-link">
                <arguments>
                    <argument name="path" xsi:type="string">quickorder</argument>
                    <argument name="label" xsi:type="string" translate="true">Express Order</argument>
                    <argument name="sortOrder" xsi:type="number">246</argument>
                    <argument name="attributes" xsi:type="array">
                        <item name="class" xsi:type="string">express</item>
                    </argument>
                </arguments>
            </block>
        </referenceBlock> 
    </body>
</page>

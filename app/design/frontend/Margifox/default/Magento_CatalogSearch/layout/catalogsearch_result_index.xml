<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="breadcrumbs" destination="content" before="-" />
        <move element="page.main.title" destination="content" after="breadcrumbs" />
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="search.result.message" template="Magento_CatalogSearch::message.phtml" after="search.result" />
        </referenceContainer>
    </body>
</page>
